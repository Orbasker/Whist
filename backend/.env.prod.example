# Production Environment Variables
# Copy this to .env.prod and fill in your Supabase credentials

# Supabase PostgreSQL Database URL
# Format: postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres
SUPABASE_DATABASE_URL=postgresql://postgres.[project-ref]:[password]@aws-0-[region].pooler.supabase.com:6543/postgres

# Supabase Project URL
SUPABASE_URL=https://[project-ref].supabase.co

# Supabase Anon Key (public key)
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase JWT Secret (for verifying tokens)
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# API Configuration
API_V1_PREFIX=/api/v1
PROJECT_NAME=Whist Game API

# CORS (JSON array format)
CORS_ORIGINS=["https://your-domain.com","http://localhost:4200"]
